{% extends 'base.html.twig' %}

{% block title %}Nouvelle Période de Paie
{% endblock %}

{% block body %}
	<h1>Nouvelle Période de Paie</h1>

	{{ form_start(form) }}
	{{ form_row(form.type_paie) }}
	{{ form_row(form.mois) }}
	{{ form_row(form.annee) }}
	{{ form_row(form.quinzaine) }}
	{{ form_row(form.statut) }}
	<button class="btn btn-primary mt-3">Enregistrer</button>
	{{ form_end(form) }}
{% endblock %}
